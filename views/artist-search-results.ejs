<h1>Artist Search Results</h1>
<% artists.forEach(artist => { %>
  <div>
    <h2><%= artist.name %></h2>
    <% if (artist.images.length > 0) { %>
      <img src="<%= artist.images[0].url %>" alt="<%= artist.name %> Image" />
    <% } %>
    <a href="/albums/<%= artist.id %>">Show Albums</a>
  </div>
<% }) %>

